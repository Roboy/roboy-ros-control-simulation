<sdf version='1.5'>
    <model name='plate'>
        <link name='plate'>
            <pose frame=''>0 0 0 0 -0 0</pose>
            <inertial>
                <pose frame=''>0.0751723 -4.02734e-06 0.412435 0 -0 0</pose>
                <mass>13.4708</mass>
                <inertia>
                    <ixx>0.480204</ixx>
                    <ixy>-3.06614e-06</ixy>
                    <ixz>0.0113382</ixz>
                    <iyy>0.472612</iyy>
                    <iyz>-6.11694e-06</iyz>
                    <izz>0.341787</izz>
                </inertia>
            </inertial>
            <collision name='plate_collision'>
                <pose frame=''>0 0 0 0 -0 0</pose>
                <geometry>
                    <mesh>
                        <scale>1 1 1</scale>
                        <uri>model://plate/cad/plate.STL</uri>
                    </mesh>
                </geometry>
            </collision>
            <visual name='plate_visual'>
                <pose frame=''>0 0 0 0 -0 0</pose>
                <geometry>
                    <mesh>
                        <scale>1 1 1</scale>
                        <uri>model://plate/cad/plate.STL</uri>
                    </mesh>
                </geometry>
                <!--<plugin name="tendon_visualizer" filename="libtendon_visualizer.so"/>-->
            </visual>
        </link>

        <link name='weight'>
            <gravity>1</gravity>
            <pose>0 0 0.2 0 0 0</pose>
            <inertial>
                <mass>2.5</mass>
                <pose frame=''>0 0 0 0 0 0</pose>
                <inertia>
                    <ixx>1</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>1</iyy>
                    <iyz>0</iyz>
                    <izz>1</izz>
                </inertia>
            </inertial>
            <collision name='collision'>
                <geometry>
                    <box>
                        <size>.1 .1 .1</size>
                    </box>
                </geometry>
            </collision>
            <visual name='visual'>
                <geometry>
                    <box>
                        <size>.1 .1 .1</size>
                    </box>
                </geometry>
            </visual>
        </link>

        <joint name="fixtoworld" type="revolute">
            <pose>0 0 0 0 0 0</pose>
            <child>plate</child>
            <parent>world</parent>
            <axis>
                <limit>
                    <lower>0</lower>
                    <upper>0</upper>
                </limit>
                <xyz>0 0 1</xyz>
            </axis>
        </joint>

        <joint name="motor0" type="prismatic">
            <pose>0 0 0 0 0 0</pose>
            <child>weight</child>
            <parent>plate</parent>
            <axis>
                <limit>
                    <lower>-1000</lower>
                    <upper>1000</upper>
                </limit>
                <xyz>0 0 1</xyz>
            </axis>
        </joint>

        <plugin name="roboySim" filename="libroboySim.so">
            <myoMuscle name="myoMuscle">
                <link name="plate">
                    <viaPoint>0.072 0 0.513</viaPoint>
                    <viaPoint>0.060 0 0.510</viaPoint>
                </link>
                <link name="weight">
                    <viaPoint>0.0 0.0 0.05</viaPoint>
                </link>
                <motor>
                    <bemf_constant>0.001488</bemf_constant> <!--V/rpm-->
                    <torque_constant>14.2</torque_constant> <!--mNm/A-->
                    <inductance>0.118</inductance> <!--mH-->
                    <resistance>0.797</resistance> <!--Ohm-->
                    <inertiaMoment>4.09</inertiaMoment> <!--gcm^2-->
                </motor>
                <gear>
                    <ratio>53</ratio>
                    <efficiency>0.59</efficiency>
                    <inertiaMoment>0.4</inertiaMoment> <!--gcm^2-->
                </gear>
                <spindle>
                    <radius>0.005</radius> <!--m-->
                </spindle>
                <SEE>
                    <stiffness>1.0</stiffness>
                    <!-- <length>0.5</length> -->
                    <length>0.1</length>
                </SEE>
            </myoMuscle>
        </plugin>
    </model>
</sdf>
